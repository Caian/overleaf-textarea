{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "declarativeContent",
    "input"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.overleaf.com/*"],
      "js": [ "diff_match_patch.js","functions.js", "content.js", "script.js"],
      "content_security_policy": "script-src 'self' https://www.overleaf.com/project/*; object-src 'self'",
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["script.js"],
  "content_security_policy": "script-src 'self' https://*.google-analytics.com/ https://*.firebase.com https://*.firebaseio.com https://www.gstatic.com/ https://www.googleapis.com https://www.googletagmanager.com/; object-src 'self'",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
